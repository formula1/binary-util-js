{
  "ignorePatterns": ["garbage", "hidden"],
  "env": { },
  "plugins": [],
  "extends": [
    "eslint:recommended"
  ],
  "globals": {
    "require": true,
    "Set": true,
    "setTimeout": true,
    "clearTimeout": true,
    "__dirname": true,
    "window": true,
